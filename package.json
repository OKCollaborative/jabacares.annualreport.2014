{
  "name": "jabacares.annualreport.2014",
  "author": "James Quinn <jamesquinn@okcollaborative.org> (http://www.okcollaborative.org)",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prerequisites": "npm install",
    "postinstall": "echo there it goes!",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean -s && npm run lint",
    "build": "npm run build:scripts -s && npm run build:styles -s && npm run build:markup -s && npm run build:images -s",
    "build:scripts": "browserify --full-path=false -d assets/scripts/index.js > dist/jabacares.min.js",
    "build:styles": "stylus assets/styles/*.stylus -m -o dist",
    "build:markup": "jade assets/markup/index.jade -P -o dist",
    "build:images": "imagemin assets/images/* -o 7 dist/images",
    "build:scriptsDEFUNCT": "browserify -d assets/scripts/index.js -p [minifyify --compressPath . --map index.js.map --output dist/index.js.map] | hashmark -n dist/index.js -s -l 8 -m assets.json 'dist/{name}{hash}{ext}'",
    "build:stylesDEFUNCT": "stylus assets/styles/main.stylus -m -o dist/ && hashmark -s -l 8 -m assets.json dist/main.css 'dist/{name}{hash}{ext}'",
    "lint": "jshint assets/scripts/.",
    "live-reload": "live-reload --port 35729 dist/",
    "prewatch": "npm run lint",
    "watch": "npm run watch:scripts & npm run watch:styles & npm run watch:markup & npm run watch:images",
    "watch:markup": "watch 'npm run build:markup' assets/markup",
    "watch:scripts": "watch 'npm run build:scripts' assets/scripts",
    "watch:styles": "watch 'npm run build:styles' assets/styles",
    "watch:images": "watch 'npm run build:images' assets/images",
    "debug": "node-debug ./bin/www.js â€“no-debug-brk",
    "start": "browser-sync start --server dist",
    "test": "mocha --require mocha --reporter spec ./test/*.js",
    "pretest": "npm run lint",
    "server-watch": "browser-sync start --files 'dist/*' --server dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jameyquinn/jabacares.annualreport.2014.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jameyquinn/jabacares.annualreport.2014/issues"
  },
  "homepage": "https://github.com/jameyquinn/jabacares.annualreport.2014",
  "dependencies": {
    "fastclick": "^1.0.6",
    "skrollr": "git://github.com/Prinzhorn/skrollr.git"
  },
  "devDependencies": {
    "browser-sync": "^2.3.1",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "hashmark": "^2.0.1",
    "imagemin": "^3.1.0",
    "jade": "^1.9.2",
    "jshint": "^2.6.3",
    "minifyify": "^6.3.2",
    "nodemon": "^1.3.7",
    "rimraf": "^2.3.2",
    "stylus": "^0.50.0",
    "uglify-js": "^2.4.17",
    "watch": "^0.14.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {}
}
